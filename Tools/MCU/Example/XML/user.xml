            <Kern_Prios>32</Kern_Prios>
        </General>
        <A7M>
            <NVIC_Grouping>2-6</NVIC_Grouping>
            <Timer_Interval>2160000</Timer_Interval>
        </A7M>
        <STM32F767IG>
            <XTAL>25</XTAL>
            <PLLM>25</PLLM>
            <PLLN>432 </PLLN>
            <PLLP>2</PLLP>
            <PLLQ>9</PLLQ>
            <IRQ>WWDG,EXTI0</IRQ>
        </STM32F767IG>
    </RME>
    <RVM>
        <Compiler>
            <Optimization>O2</Optimization>
            <Prioritization>Time</Prioritization>
        </Compiler>
        <General>
            <Code_Size>0x1000</Code_Size>
            <Data_Size>0x1000</Data_Size>
            <Extra_Captbl>128</Extra_Captbl>
            <Recovery>Process</Recovery>
        </General>
        <VMM>


    
        </VMM>
    </RVM>
    <Process>                                                                   <!-- The user compartments -->
        <P1>
            <General>
                <Name>Domain1</Name>
                <Extra_Captbl>9</Extra_Captbl>                                  <!-- Size of the capability table, can be a number or something else -->
            </General>
            <Compiler>
                <Optimization>O2</Optimization>
                <Prioritization>Time</Prioritization>
            </Compiler>
            <Memory>                                                            <!-- These memory are mapped in by the booter and is definitely not managed by this component -->
                <M1>
                    <Start>0x20000000</Start>
                    <Size>0x10000</Size>
                    <Type>CODE/DATA/DEV</Type>                                  <!-- Where your code goes to -->
                    <Attribute>RWXBCS</Attribute>                               <!-- Read, Write, Execute, Bufferable, Cacheable, Static -->
                </M1>
            </Memory>
            <Thread>
                <T1>
                    <Name>Motor_Control</Name>
                    <Entry>Func_Calc</Entry>
                    <Stack_Addr>0x20003000/AUTO</Stack_Addr>
                    <Stack_Size>0x1000</Stack_Size>
                    <Parameter>0x1234</Parameter>
                    <Priority>3</Priority>
                </T1>
            </Thread>
            <Invocation>
                <I1>
                    <Name>Motor_Adjust</Name>
                    <Entry>Func_Adjust</Entry>
                    <Stack_Addr>0x20004000/Auto</Stack_Addr>
                    <Stack_Size>0x1000</Stack_Size>
                </I1>
            </Invocation>
            <Ports>
                <P1>
                    <Name>Get_UI_Data</Name>
                    <Process>Domain1</Process>
                </P1>
            </Ports>
            <Endpoint>
                <E1>
                     <Name>EP_Motor</Name>
                     <Type>Send/Receive/Handler</Type>
                     <Process>Domain1</Process>
                </E1>
            </Endpoint>
        </P1>
    </Process>
