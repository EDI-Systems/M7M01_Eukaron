<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<Project> 
    <Name>M7M1</Name>
    <Platform>A7M</Platform>
    <Chip>STM32F767IG</Chip>
    <Fullname>STM32F767IGT6</Fullname>
    <RME>
        <Compiler>
            <Optimization>O2</Optimization>
            <Prioritization>Time</Prioritization>
        </Compiler>
        <General>
            <Code_Start>0x08000000</Code_Start>
            <Code_Size>0x20000</Code_Size>
            <Data_Start>0x20000000</Data_Start>
            <Data_Size>0x10000</Data_Size>
            <Extra_Kmem>0x8000</Extra_Kmem>
